#![feature(proc_macro_hygiene, decl_macro)]

#[macro_use] extern crate rocket;
use rocket_contrib::templates::Template;
use rocket::response::NamedFile;

#[get("/")]
fn index() -> io::Result<NamedFile> {
	NamedFile::op
}

fn main() {
    rocket::ignite()
    	.mount("/", routes![index])
    	.attach(Template::fairing())
    	.launch();
}
